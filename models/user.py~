#!/usr/bin/env python3
"""Class User"""
from models.base_model import BaseModel


class User(BaseModel):
    """This class create users"""

    email = ""
    password = ""
    first_name = ""
    last_name = ""

    def __init__(self, *args, **kwargs):
        """inherits the superclass"""
        super().__init__(*args, **kwargs)
